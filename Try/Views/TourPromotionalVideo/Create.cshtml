@model Try.Models.TourPromotionalVideos

<h2>Add New Video</h2>
<form asp-action="Create" enctype="multipart/form-data">
    <div class="form-group">
        <label>Title</label>
        <input asp-for="VideoTitle" class="form-control" required>
    </div>
    <div class="form-group">
        <label>Upload Video</label>
        <input type="file" asp-for="VideoURL" class="form-control">
    </div>
    <button type="submit" class="btn btn-success">Save</button>
</form>
